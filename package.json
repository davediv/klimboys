{
	"name": "SVDashboard",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "npm run build && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:generate": "drizzle-kit generate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:migrate": "wrangler d1 migrations apply klimboys --local",
		"db:remote:migrate": "wrangler d1 migrations apply klimboys --remote",
		"db:seed": "wrangler d1 execute klimboys --local --file=drizzle/seed.sql",
		"db:remote:seed": "wrangler d1 execute klimboys --remote --file=drizzle/seed.sql",
		"db:rollback": "wrangler d1 execute klimboys --local --file=drizzle/rollback.sql",
		"db:remote:rollback": "wrangler d1 execute klimboys --remote --file=drizzle/rollback.sql",
		"deploy": "wrangler pages deploy build",
		"log": "wrangler pages deployment tail",
		"cf-typegen": "wrangler types ./src/worker-configuration.d.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-cloudflare": "^7.0.4",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^22",
		"daisyui": "^5.0.43",
		"drizzle-kit": "^0.30.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"wrangler": "^4.19.2"
	},
	"dependencies": {
		"@lucide/svelte": "^0.525.0",
		"better-auth": "^1.2.9",
		"chart.js": "^4.5.0",
		"drizzle-orm": "^0.40.0",
		"zod": "^3.25.67"
	}
}
